{
    "manifest_version": 2,
    "name": "Bulgakov Extension",
    "description": "Changes the style of the site.",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": ["*://ithub.bulgakov.app/*"],
        "css": [
          "darkTheme.css"
        ],
        "js": ["hsl.js", "changeCss.js"]
      }
    ]
}